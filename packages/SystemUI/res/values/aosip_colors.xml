<resources>
    <!-- Floating action button -->
    <color name="fab_color">@*android:color/accent_device_default_light</color>
    <color name="floating_action_button_icon_color">@android:color/white</color>

    <!-- Theme tile -->
    <color name="quick_settings_theme_tile_default">#4285F4</color>
    <color name="quick_settings_theme_tile_red">#FF5252</color>
    <color name="quick_settings_theme_tile_pink">#FF4081</color>
    <color name="quick_settings_theme_tile_purple">#E040FB</color>
    <color name="quick_settings_theme_tile_deep_purple">#7C4DFF</color>
    <color name="quick_settings_theme_tile_indigo">#536DFE</color>
    <color name="quick_settings_theme_tile_blue">#448AFF</color>
    <color name="quick_settings_theme_tile_light_blue">#03A9F4</color>
    <color name="quick_settings_theme_tile_cyan">#00BCD4</color>
    <color name="quick_settings_theme_tile_teal">#009688</color>
    <color name="quick_settings_theme_tile_green">#4CAF50</color>
    <color name="quick_settings_theme_tile_light_green">#8BC34A</color>
    <color name="quick_settings_theme_tile_lime">#CDDC39</color>
    <color name="quick_settings_theme_tile_yellow">#FFEB3B</color>
    <color name="quick_settings_theme_tile_amber">#FFC107</color>
    <color name="quick_settings_theme_tile_orange">#FF9800</color>
    <color name="quick_settings_theme_tile_deep_orange">#FF5722</color>
    <color name="quick_settings_theme_tile_brown">#795548</color>
    <color name="quick_settings_theme_tile_grey">#9E9E9E</color>
    <color name="quick_settings_theme_tile_blue_grey">#607D8B</color>
    <color name="quick_settings_theme_tile_black">#000000</color>
    <color name="quick_settings_theme_tile_white">#ffffff</color>
    <color name="quick_settings_theme_tile_user_one">#900000</color>
    <color name="quick_settings_theme_tile_user_two">#42069a</color>
    <color name="quick_settings_theme_tile_user_three">#1976D2</color>
    <color name="quick_settings_theme_tile_user_four">#42f4aa</color>
    <color name="quick_settings_theme_tile_user_five">#0EE898</color>
    <color name="quick_settings_theme_tile_user_six">#FFC27B</color>
    <color name="quick_settings_theme_tile_user_seven">#FFCC00</color>

    <!-- Custom Carrier Label -->
    <color name="status_bar_carrier_color">#FFFFFFFF</color>

    <!-- Screenshot action colors -->
    <color name="screenshot_flash_color">#b3adadad</color> <!-- light grey with some transparency -->
    <color name="screenshot_background_color">@android:color/black</color>

    <!-- QS tiles Active, Inactive and Unavailable tint (Requires config_enable_qs_tile_tinting bool set to true) -->
    <color name="qs_tiles_active_tint">@*android:color/primary_device_default_settings_light</color>
    <color name="qs_tiles_inactive_tint">@*android:color/text_color_secondary</color>
    <color name="qs_tiles_unavailable_tint">@*android:color/text_color_secondary</color> <!-- keep in mind that the code adds 0.26 (light) or 0.30 (dark) alpha to this color. -->

    <!-- QS tiles background Active, Disable, and Inactive tint (Requires config_enable_qs_tile_tinting bool set to true) -->
    <color name="qs_tiles_background_active_tint">@*android:color/accent_device_default_light</color>
    <color name="qs_tiles_background_disabled_tint">@*android:color/secondary_text_material_light</color> <!-- keep in mind that the code adds 0.26 (light) or 0.30 (dark) alpha to this color. -->
    <color name="qs_tiles_background_inactive_tint">@*android:color/text_color_secondary</color>

    <!-- QS tiles expand, underline and label colors -->
    <color name="qs_tiles_expand_tint">@*android:color/text_color_primary</color>
    <color name="qs_tiles_label_color">@*android:color/text_color_primary</color>
    <color name="qs_tiles_secondary_label_color">@*android:color/text_color_secondary</color>
    <color name="qs_tiles_underline_color">@*android:color/foreground_material_light</color>

</resources>
